{
	"editor.detectIndentation": false,
	"tabnine.experimentalAutoImports": true,
	/* "[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}, */
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"gitlens.advanced.messages": {
		"suppressGitDisabledWarning": true
	},
	"terminal.integrated.automationShell.windows": "",
	"terminal.integrated.shellArgs.windows": ["-ExecutionPolicy", "Bypass"],
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"mssql.connections": [
		{
			"server": "{{put-server-name-here}}",
			"database": "{{put-database-name-here}}",
			"user": "{{put-username-here}}",
			"password": "{{put-password-here}}"
		}
	],
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed",
			"extensionName": ".min.css",
			"savePath": "/dist/css"
		}
	],
	"liveServer.settings.donotShowInfoMsg": true,
	"workbench.iconTheme": "material-icon-theme",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"prettier.printWidth": 180,
	"liveServer.settings.donotVerifyTags": true,
	"editor.renderWhitespace": "all",
	"stylelint.packageManager": "yarn",
	"yaml.schemas": {
		"file:///Users/a038/.vscode/extensions/atlassian.atlascode-2.9.1/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
	},
	"redhat.telemetry.enabled": true,
	"workbench.colorTheme": "Cyberpunk",
	"terminal.external.osxExec": "iTerm.app",
	"editor.bracketPairColorization.enabled": true,
	"explorer.compactFolders": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"php.validate.executablePath": "/opt/homebrew/etc/php/8.1/",
	"editor.largeFileOptimizations": false,
	"editor.insertSpaces": false,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.codeActionsOnSave": {},
	"cSpell.userWords": ["draftjs", "fontfaceobserver"],
	"prettier.trailingComma": "none",
	"npm.keybindingsChangedWarningShown": true,
	"editor.inlineSuggest.enabled": true,
	"material-icon-theme.folders.associations": {
		"domain": "Content",
		"infrastructure": "Server",
		"ui": "Theme",
		"application": "Client",
		"mock-server": "Server"
	},
	"workbench.layoutControl.enabled": false,
	"editor.formatOnSave": true,
	"nxConsole.showNodeVersionOnStartup": false,
	"githubPullRequests.pullBranch": "never",
	"[yaml]": {
		"editor.autoIndent": "advanced"
	},
	"[yml]": {
		"editor.autoIndent": "advanced"
	},
	"prettier.requireConfig": true,
	"eslint.format.enable": true
}
